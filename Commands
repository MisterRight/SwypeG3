public class Commands {
  
    private String message;
    private String MOTD;
    
    public void setUserMessage(String newMessage){
        this.message = newMessage;
    }
    
    public String getMessage(){
        return this.message;
    }
    
    public void setMOTD(String newMOTD){
        this.MOTD = newMOTD;
    }
    
    public String getMOTD(){
        return this.MOTD;
    }
}
